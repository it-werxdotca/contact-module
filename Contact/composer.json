{
    "name": "it-werxdotca/laravel-contact-module",
    "description": "A simple contact module for nwidart/laravel-modules. It stores information sent from the contact form into an SQL table, sends an email to the sender and a configurable email address.",
    "type": "laravel-module",
    "authors": [
        {
            "name": "Ron Mac Quarrie",
            "email": "coding.laravel@it-werx.ca"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Contact\\Providers\\ContactServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Contact\\": "",
            "Modules\\Contact\\App\\": "app/",
            "Modules\\Contact\\Database\\Factories\\": "database/factories/",
            "Modules\\Contact\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Contact\\Tests\\": "tests/"
        }
    }
}
